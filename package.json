    "@react-native/metro-config": "^0.72.11",
    "@tanstack/eslint-plugin-query": "^4.32.5",
    "@tsconfig/react-native": "^3.0.0",
    "@types/color": "^3.0.4",
    "@types/rails__actioncable": "^6.1.6",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
